<div class="page-header">
  <h1>Poslední události
    <small> Co Vám neuniklo o vašich partnerech</small>
  </h1>
</div>

<ul>
  <% @news_list.each do |n| %>
      <% next unless n.owner %>
      <% if n.owner_type == 'AresRegistration' %>
          <li>
            <%= I18n.l n.created_at %>
                ič: <%= n.owner.ic rescue '' %>
            <% if n.action == 'create' %>
                <span class="label"><%= n.owner.name rescue '' %></span>:
                Přidána mezi kontrolované
            <% else %>
                <span class="label label-info"><%= n.owner.name rescue '' %></span>:
                Změny v evidenci:
                <%= n.audited_changes  %>
            <% end %>
          </li>


          </li>
      <% end %>
  <% end %>
</ul>

